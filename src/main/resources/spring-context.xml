<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="questRepo" class="ru.otus.dao.repository.QuestRepository">
        <property name="filePath" value="src/main/resources/questionnaire.csv" />
    </bean>

    <bean id="questService" class="ru.otus.service.impl.QuestServiceImpl">
        <constructor-arg ref="questRepo"/>
    </bean>

</beans>
